{
  "name": "things-to-do",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start:app:watch": "yarn workspace @ttd/app start",
    "start:server:watch": "yarn workspace @ttd/server start:dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@ttd/app": "workspace:^",
    "eslint-config-custom": "*",
    "path": "^0.12.7",
    "prettier": "latest",
    "turborepo": "^0.0.1",
    "typescript": "^4.8.2",
    "workspace": "^0.0.1-preview.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@ttd/server": "workspace:^",
    "mongoose-lean-virtuals": "^0.9.1",
    "vite": "2.9.12"
  },
  "packageManager": "yarn@3.2.4"
}
