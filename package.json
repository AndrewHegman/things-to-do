{
  "name": "things-to-do",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start:app:watch": "yarn workspace @ttd/app start",
    "start:server:watch": "yarn workspace @ttd/server start:dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": "16.13.1",
    "npm": "^8.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
